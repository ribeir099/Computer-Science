CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    /*
    Nand(a=a, b=b, out=temp1);
    Nand(a=a, b=temp1, out=temp2);
    Nand(a=temp1, b=b, out=temp3);
    Nand(a=temp2, b=temp3, out=out);]
    */

    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=a, b=notb, out=aAndNotb);
    And(a=nota, b=b, out=notaAndb);
    Or(a=aAndNotb, b=notaAndb, out=out);
}
